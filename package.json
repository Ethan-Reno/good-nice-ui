    "tslib": "^2.5.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-tsconfig-paths": "^4.0.5",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-hook-form": ">=7.43.0"
  }
