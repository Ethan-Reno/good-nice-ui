{
  "name": "lift-companion-ui",
  "version": "0.2.4",
  "description": "UI test",
  "author": "Ethan Reno",
  "license": "MIT",
  "private": false,
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    "import": "./dist/index.js"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Ethan-Reno/lift-companion-ui"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:styles": "postcss src/tailwind.css -o dist/index.css",
    "declarations": "tsc -p tsconfig.json",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts, .tsx' --out-dir 'dist/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts, .tsx' --out-dir 'dist/cjs' --source-maps",
    "build:bundles": "rollup -c",
    "build": "npm-run-all -l clean -p build:bundles declarations",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@radix-ui/react-accessible-icon": "^1.0.1",
    "@radix-ui/react-avatar": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-visually-hidden": "^1.0.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-a11y": "^7.0.0-beta.53",
    "@storybook/addon-essentials": "^7.0.0-beta.53",
    "@storybook/addon-interactions": "^7.0.0-beta.53",
    "@storybook/addon-links": "^7.0.0-beta.53",
    "@storybook/blocks": "^7.0.0-alpha.8",
    "@storybook/manager-api": "^7.0.0-beta.53",
    "@storybook/react": "^7.0.0-beta.53",
    "@storybook/react-vite": "^7.0.0-beta.53",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/theming": "^7.0.0-beta.53",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "chromatic": "^6.17.0",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.15",
    "lucide-react": "^0.115.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "storybook": "^7.0.0-beta.53",
    "tailwindcss": "^3.2.7",
    "tailwindcss-animate": "^1.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
