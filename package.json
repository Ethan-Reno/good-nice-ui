{
  "name": "lift-companion-ui",
  "version": "0.3.0",
  "description": "UI test",
  "author": "Ethan Reno",
  "license": "MIT",
  "private": false,
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Ethan-Reno/lift-companion-ui"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:styles": "postcss src/tailwind.css -o dist/index.css",
    "build:types": "tsc -p tsconfig.json",
    "build:bundle": "rollup -c",
    "build": "npm-run-all -l build:clean -p build:bundle build:types build:styles",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "dependencies": {
    "@radix-ui/react-accessible-icon": "^1.0.1",
    "@radix-ui/react-avatar": "^1.0.2",
    "@radix-ui/react-checkbox": "^1.0.4",
